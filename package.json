{
  "name": "ux-components",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-ux-components-lib": "ng build @netex/ux-components",
    "build-ux-components-themes": "sass-pack -s ./projects/netex/ux-components/src/themes/*.theme.scss -o ./dist/netex/ux-components/themes",
    "build-ux-components-images": "mkdir -p ./dist/netex/ux-components/assets && cp -R ./src/assets/images ./dist/netex/ux-components/assets",
    "build-ux-components-fonts-icons": "mkdir -p ./dist/netex/ux-components/assets && cp -R ./src/assets/fonts ./dist/netex/ux-components/assets",
    "build-ux-components-styles": "sass-pack -s ./projects/netex/ux-components/src/styles/styles.scss -o ./dist/netex/ux-components/styles/",
    "build-ux-components": "rm -rf dist/netex/ux-components && npm run build-ux-components-lib && npm run build-ux-components-themes && npm run build-ux-components-images && npm run build-ux-components-styles && npm run build-ux-components-fonts-icons"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/router": "^6.1.9",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.3.3",
    "sass-pack": "^2.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.3",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.4",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "ng-packagr": "^4.1.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
